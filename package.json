{
  "name": "frame-monorepo",
  "version": "0.2.1",
  "private": true,
  "packageManager": "pnpm@8.7.1",
  "description": "frame-monorepo",
  "author": "guygubaby@gmail.com",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "build": "pnpm -r --parallel --filter !playground run build",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "pnpm lint -- --fix",
    "play": "pnpm -C playground run dev",
    "parse-yaml": "esno scripts/parse-yaml.ts",
    "build:play": "pnpm -C playground run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest",
    "typecheck": "tsc ./packages/**/*.ts --noEmit "
  },
  "dependencies": {
    "@bryce-loskie/scrollbar": "^0.1.3"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.2.0",
    "@bryce-loskie/utils": "^0.3.0",
    "@iconify-json/carbon": "^1.1.21",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vueuse/core": "^10.4.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "jsdom": "^22.1.0",
    "motion": "^10.16.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.55.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "yaml": "^2.3.2"
  }
