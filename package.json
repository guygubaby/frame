    "@iconify-json/carbon": "^1.1.16",
    "@types/node": "^20.2.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.1.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "jsdom": "^22.0.0",
    "motion": "^10.16.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "unocss": "^0.52.0",
    "unplugin-auto-import": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.8",
    "vite-plugin-inspect": "^0.7.26",
    "vite-plugin-pages": "^0.30.1",
    "vitest": "^0.31.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "yaml": "^2.2.2"
  }
