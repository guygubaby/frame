    "@iconify-json/carbon": "^1.1.33",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10.9.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "jsdom": "^24.0.0",
    "motion": "^10.17.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-pages": "^0.32.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "yaml": "^2.4.2"
  }
